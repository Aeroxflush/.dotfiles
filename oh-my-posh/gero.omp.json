{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "user",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#000000"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#000000",
          "properties": {
            "text": " ->"
          }
        }
      ]
    }
  ],
  "final_space": true
}
